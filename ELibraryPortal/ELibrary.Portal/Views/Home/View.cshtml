@model AppFileFilterModel

@{
    ViewData["Title"] = "View";
}

<h2>View</h2>

<h4>AppFileFilterModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="View">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="File" class="control-label">File</label>
                <input type="file" asp-for="File" class="file file-actions form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

